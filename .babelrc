{
  "plugins": [
    "transform-runtime"
  ],
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [
            {
              "transform"  : "react-transform-hmr",
              "imports" : ["react"],
              "locals"  : ["module"]
            }
          ]
        }]
      ]
    }
  }
}
