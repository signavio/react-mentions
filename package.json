{
  "name": "react-mentions",
  "version": "0.2.9",
  "description": "React mentions input",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha",
    "transpile": "NODE_ENV=production babel src --out-dir lib",
    "dist": "NODE_ENV=production browserify lib/index.js -t reactify -t browserify-shim -t envify --detect-globals false -s ReactMentions > dist/react-mentions.js",
    "clean": "rimraf dist && rimraf lib",
    "start": "cd gh-pages && webpack-dev-server --config webpack.dev.config.js --port 9090 --d --hot --inline --progress --colors",
    "bump-patch": "mversion patch -m",
    "bump-minor": "mversion minor -m",
    "bump-major": "mversion major -m",
    "pretranspile": "npm run clean",
    "predist": "npm run transpile && mkdirp dist",
    "postdist": "uglifyjs dist/react-mentions.js --compress warnings=false > dist/react-mentions.min.js",
    "prepublish": "npm run dist",
    "prepages-publish": "cd gh-pages && webpack",
    "pages-publish": "grunt publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/effektif/react-mentions.git"
  },
  "keywords": [
    "react",
    "mentions",
    "react-component"
  ],
  "author": "Jan-Felix Schwarz",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/effektif/react-mentions/issues"
  },
  "homepage": "https://github.com/effektif/react-mentions",
  "devDependencies": {
    "babel": "^6.5.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^6.1.1",
    "browserify": "4.2.3",
    "browserify-shim": "3.6.0",
    "bundle-loader": "0.5.1",
    "chai": "^3.0.0",
    "chai-enzyme": "^0.4.1",
    "chai-things": "^0.2.0",
    "css-loader": "^0.20.0",
    "envify": "1.2.0",
    "enzyme": "^2.0.0",
    "grunt": ">=0.4.1",
    "grunt-contrib-copy": ">=0.4.1",
    "grunt-contrib-less": "~1.0.0",
    "grunt-gh-pages": "~0.9.1",
    "isparta": "^4.0.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.0.4",
    "less": "~2.3.1",
    "less-loader": "^2.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^1.20.1",
    "mversion": "^1.10.0",
    "react-addons-test-utils": "^0.14.7",
    "react-router": "^1.0.0-rc3",
    "react-tools": "~0.14.0-alpha3",
    "react-transform-hmr": "^1.0.2",
    "reactify": "0.15.x",
    "rf-release": "0.3.2",
    "rimraf": "^2.5.2",
    "sinon": "~1.12.1",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.12.4",
    "uglify-js": "2.4.15",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "browserify-shim": {
    "react": "global:React",
    "react/addons": "global:React",
    "react-dom": "global:ReactDOM"
  },
  "dependencies": {
    "babel-runtime": "^6.5.0",
    "lodash": "^4.5.1",
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0",
    "substyle": "^1.1.1"
  }
}
